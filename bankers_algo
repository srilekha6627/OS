#include<stdio.h>

int main(){
    int n, r, rc, pc=0, i, j, k, flag=1;

    printf("Enter total no:of processes: ");
    scanf("%d", &n);

    printf("Enter total no:of resources: ");
    scanf("%d", &r);

    int avail[r], max[n][r], alloc[n][r], need[n][r], status[n];

    printf("Enter max resources needed for each process\n");
    for(i= 0; i<n; i++){
        for(j= 0; j<r; j++){
            scanf("%d", &max[i][j]);
        }
    }

    printf("Enter resources Allocated for each process\n");
    for(i= 0; i<n; i++){
        for(j= 0; j<r; j++){
            scanf("%d", &alloc[i][j]);
        }
    }

    printf("Enter no:of available resources\n");
    for(i= 0; i<r; i++){
        scanf("%d", &avail[i]);
    }
    printf("\n");

    for(i= 0; i<n; i++){
        for(j= 0; j<r; j++){
            need[i][j]= max[i][j]- alloc[i][j];
            status[i]= 0;
        }
    }

    k=0;

    while(flag){
        flag=0;
        for(k=0; k<n;k++){
            rc=0;
            for(i= 0; i<r; i++){
                if(status[k]==0 && need[k][i]<= avail[i]){
                    rc+=1;
                }
            }

            if(rc==r){
                status[k]=1;
                for(j= 0; j< r; j++){
                    avail[j]+= alloc[k][j];
                }
                pc+= 1;
                printf("p-> %d\n", k+1);
                flag=1;
            }
        }
    }

    if(pc==n){
        printf("Safe state");
    }
    else{
        printf("Dead lock");
    }

    return 0;
}
